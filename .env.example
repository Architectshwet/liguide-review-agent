# PostgreSQL Database Configuration
POSTGRES_HOST=host.docker.internal
POSTGRES_PORT=5432
POSTGRES_DB=liquide_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Application Configuration
PYTHONPATH=/app
PYTHONUNBUFFERED=1

# Checkpointer Configuration
CHECKPOINTER_TYPE=postgres
USE_LOCAL_CHECKPOINTER=1

# Session Store Debugging Sync Configuration
ENABLE_SESSION_SYNC=true

# LangGraph Configuration
LANGGRAPH_DEV=true

# OpenAI configuration
OPENAI_API_KEY=
OPENAI_MODEL_REVIEW_ANALYST="gpt-5.1"
OPENAI_TEMPERATURE_REVIEW_ANALYST="0.2"

# SerpAPI configuration (required for live review fetch)
SERPAPI_API_KEY=

# Live review sources (Google Play + App Store)
GOOGLE_PLAY_APP_ID=life.liquide.app
APPLE_PRODUCT_ID=1624726081
APPLE_COUNTRY=in

# Langfuse configuration
LANGFUSE_ENABLED=true
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=

# CORS allowlist (comma-separated origins)
CORS_ALLOWED_ORIGINS=http://localhost:3000
# Qdrant settings
QDRANT_PATH=./qdrant_vectordb
QDRANT_COLLECTION_NAME=liquide_reviews
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
